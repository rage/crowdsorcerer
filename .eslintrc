{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "settings": {
      "import/resolver": "webpack"
  },
  "rules": {
      "no-console": ["error", { "allow": ["warn", "error", "info"] }],
      "max-len": ["error", { "code": 128 }],
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "react/prefer-stateless-function": 0,
      "react/prop-types": 0,
      "class-methods-use-this": 0,
      "no-plusplus": 0
  },
  "ignore": ["node_modules"],
  "ext": [".js", ".jsx"]
}
