{
  "name": "crowdsorcerer",
  "version": "0.1.11",
  "main": "dist/app.js",
  "files": [
    "dist"
  ],
  "author": "Henrik Nygren <nygrenh@gmail.com>",
  "license": "MIT OR Apache-2.0",
  "scripts": {
    "start": "env NODE_ENV=development yarn run webpack-dev-server --  --progress --colors --port 3001",
    "webpack": "env NODE_ENV=development node ./node_modules/.bin/webpack -p",
    "build": "rm -rf dist && NODE_ENV=production node ./node_modules/.bin/webpack -p",
    "lint": "yarn run eslint -- --ext .js --ext .jsx",
    "test": "yarn run nyc ava",
    "testrprt": "./node_modules/.bin/nyc report --reporter=html; google-chrome coverage/index.html",
    "coverage": "BABEL_ENV=production babel --out-dir=dist index.jsx",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@skidding/react-codemirror": "^1.0.0",
    "actioncable": "^5.1.1",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "codemirror": "^5.26.0",
    "core-js": "^2.6.1",
    "draft-js": "^0.10.1",
    "file-loader": "^0.11.1",
    "likert-react": "^0.0.0-beta5",
    "react": "^16.3.0",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^16.3.0",
    "react-icons": "^2.2.5",
    "react-motion": "^0.5.0",
    "react-motion-ui-pack": "^0.10.2",
    "react-redux": "^5.0.4",
    "react-tag-autocomplete": "^5.4.1",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.6.0",
    "redux-action-analytics": "^0.0.2",
    "redux-create-reducer": "^1.1.1",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "slate": "^0.20.3",
    "store": "^1.3.20",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@material-ui/core": "^3.8.3",
    "app-module-path": "^2.2.0",
    "autoprefixer": "^7.0.1",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "browser-env": "^2.0.31",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.45.0",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.9.0",
    "nyc": "^10.3.2",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^2.0.3",
    "sass-loader": "^6.0.3",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "@material-ui/core": "^3.8.3"
  },
  "ava": {
    "files": [
      "./src/**/__tests__/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./resolve-modules.js",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "*-test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
