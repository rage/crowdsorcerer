{
  "name": "crowdsorcerer",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Henrik Nygren <nygrenh@gmail.com>",
  "license": "MIT OR Apache 2",
  "scripts": {
    "start": "env NODE_ENV=development yarn run webpack-dev-server --  --progress --colors",
    "webpack": "env NODE_ENV=development node ./node_modules/.bin/webpack -p",
    "build": "rm -rf dist && NODE_ENV=production node ./node_modules/.bin/webpack",
    "lint": "yarn run eslint -- --ext .js --ext .jsx",
    "test": "yarn run nyc ava",
    "coverage": "BABEL_ENV=production babel --out-dir=dist index.jsx"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "codemirror": "^5.26.0",
    "file-loader": "^0.11.1",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-bootstrap": "^0.31.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "reactstrap": "^4.6.2",
    "redux": "^3.6.0",
    "redux-create-reducer": "^1.1.1",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "autoprefixer": "^7.0.1",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.45.0",
    "node-sass": "^4.5.1",
    "nyc": "^10.3.2",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^2.0.3",
    "sass-loader": "^6.0.3",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "ava": {
    "files": [
      "./src/**/__tests__/*.js"
    ],
    "require": [
      "babel-register",
      "./resolve-modules.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "*-test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
